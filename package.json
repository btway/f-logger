  "bugs": {
    "url": "https://github.com/timmyichen/f-logger/issues"
  },
  "homepage": "https://github.com/timmyichen/f-logger#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "consolidate": "^0.15.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "nunjucks": "^3.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
